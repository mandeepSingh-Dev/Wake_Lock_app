<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_grapj"
    app:startDestination="@id/splash_Fragment">

    <fragment
        android:id="@+id/splash_Fragment"
        android:name="com.example.wake_lockapp.ui.fragments.Splash_Fragment"
        android:label="fragment_splash_"
        tools:layout="@layout/fragment_splash_" >
        <action
            android:id="@+id/action_splash_Fragment_to_home_fragment"
            app:destination="@id/home_fragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/to_right"
            app:popExitAnim="@anim/from_left"
            />
    </fragment>
    <fragment
        android:id="@+id/home_fragment"
        android:name="com.example.wake_lockapp.ui.fragments.Home_fragment"
        android:label="fragment_home_fragment"
        tools:layout="@layout/fragment_home_fragment"
        >

    </fragment>
</navigation>